"""
Multiple Inheritance Service Client
"""
@resource("/multipleInheritance/feline")
interface MultipleInheritanceFeline {
  """
  Get a feline where meows and hisses are true
  """
  @operationId("getFeline")
  list(): Ok<Feline> | Error;

  """
  Put a feline who hisses and doesn't meow
  """
  @operationId("putFeline")
  createOrUpdate(
