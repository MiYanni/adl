model OkEmpty {
  @header statusCode: 200;
}

model Ok<T> {
  @header statusCode: 200;
  ... T;
}

model Error {
  status: int32;
  message: string;
}

model Pet {
  name: string;
}

model Feline {
  meows: boolean;
  hisses: boolean;
}

model Cat {
  ... Pet;
  ... Feline;
  likesMilk: boolean;
}

model Kitten {
  ... Cat;
  eatsMiceYet: boolean;
}

model Horse {
  ... Pet;
  isAShowHorse: boolean;
}

"""
Multiple Inheritance Service Client
"""
@resource("/multipleInheritance/horse")
interface MultipleInheritanceHorse {
  """
  Get a horse with name 'Fred' and isAShowHorse true
  """
  @operationId("getHorse")
  list(): Ok<Horse> | Error;

  """
  Put a horse with name 'General' and isAShowHorse false
  """
  @operationId("putHorse")
  createOrUpdate(
    horse: Horse
  ): Ok<string> | Error;
}

"""
Multiple Inheritance Service Client
"""
@resource("/multipleInheritance/pet")
interface MultipleInheritancePet {
  """
  Get a pet with name 'Peanut'
  """
  @operationId("getPet")
  list(): Ok<Pet> | Error;

  """
  Put a pet with name 'Butter'
  """
  @operationId("putPet")
