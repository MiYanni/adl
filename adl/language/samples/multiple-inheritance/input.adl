"""
Multiple Inheritance Service Client
"""
@resource("/multipleInheritance/cat")
interface MultipleInheritanceCat {
  """
  Get a cat with name 'Whiskers' where likesMilk, meows, and hisses is true
  """
  @operationId("getCat")
  list(): Ok<Cat> | Error;

  """
  Put a cat with name 'Boots' where likesMilk and hisses is false, meows is true
  """
  @operationId("putCat")
  createOrUpdate(
    cat: Cat
  ): Ok<string> | Error;
}

"""
Multiple Inheritance Service Client
"""
@resource("/multipleInheritance/kitten")
interface MultipleInheritanceKitten {
  """
  Get a kitten with name 'Gatito' where likesMilk and meows is true, and hisses and eatsMiceYet is false
  """
  @operationId("getKitten")
  list(): Ok<Kitten> | Error;

  """
  Put a kitten with name 'Kitty' where likesMilk and hisses is false, meows and eatsMiceYet is true
  """
  @operationId("putKitten")
  createOrUpdate(
    kitten: Kitten
  ): Ok<string> | Error;
}