model OkEmpty {
  @header statusCode: 200;
}

model Ok<T> {
  @header statusCode: 200;
  ... T;
}

model Error {
  status: int32;
  message: string;
}

// boolean literals don't work
model TrueValue = "true";
model FalseValue = "false";

"""
Test Infrastructure for AutoRest
"""
@resource("/bool/true")
interface BoolTrue {
  """
  Get true Boolean value
  """
  // x-ms-examples, Ok/Error description
  @operationId("bool_getTrue")
  //@tags(["Bool Operations"])
  list(): Ok<TrueValue> | Error;

  """
  Set Boolean value true
  """
  // x-ms-examples, Ok/Error description
  @operationId("bool_putTrue")
  //@tags(["Bool Operations"])
  createOrUpdate(
    @body
    boolBody: TrueValue
  ): OkEmpty | Error;
}

"""
Test Infrastructure for AutoRest
"""
@resource("/bool/false")
interface BoolFalse {
  """
  Get false Boolean value
  """
  @operationId("bool_getFalse")
  list(): Ok<FalseValue> | Error;

  """
  Set Boolean value true
  """
  @operationId("bool_putFalse")
  createOrUpdate(
    @body
    boolBody: FalseValue
  ): OkEmpty | Error;
}

"""
Test Infrastructure for AutoRest
"""
@resource("/bool/null")
interface BoolNull {
  """
  Get null Boolean value
  """
  // Cannot make nullable boolean
  @operationId("bool_getNull")
  list(): Ok<boolean> | Error;
}

"""
Test Infrastructure for AutoRest
"""
@resource("/bool/invalid")
interface BoolInvalid {
  """
  Get invalid Boolean value
  """
  @operationId("bool_getInvalid")
  list(): Ok<boolean> | Error;
}