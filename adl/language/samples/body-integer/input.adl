interface IntUnixTime {
  """
  Get datetime encoded as Unix time value
  """
  // No unixtime type
  @operationId("int_getUnixTime")
  list(): Ok<int64> | Error;

  """
  Put datetime encoded as Unix time
  """
  // Adds createOrUpdate to path and uses GET
  @operationId("int_getUnixTime")
  createOrUpdate(
    """
    int body
    """
    // Note: Default to @body
