interface IntMax32 {
  """
  Put max int32 value
  """
  // Adds createOrUpdate to path and uses GET
  @operationId("int_putMax32")
  createOrUpdate(
    """
    int body
    """
    // Note: Default to @body
    intBody: int32
  ): OkEmpty | Error;
}

