model OkEmpty {
  @header statusCode: 200;
}

model Ok<T> {
  @header statusCode: 200;
  ... T;
}

model Error {
  status: int32;
  message: string;
}

"""
Test Infrastructure for AutoRest
"""
@resource("/int/null")
interface IntNull {
  """
  Get null Int value
  """
  // nullable response type
  @operationId("int_getNull")
  list(): Ok<int32> | Error;
}

@resource("/int/invalid")
interface IntInvalid {
  """
  Get invalid Int value
  """
  @operationId("int_getInvalid")
  list(): Ok<int32> | Error;
}

